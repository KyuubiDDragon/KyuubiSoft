# Application
APP_ENV=development
APP_DEBUG=true
APP_URL=http://localhost

# Database
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=kyuubisoft
DB_USERNAME=kyuubisoft
DB_PASSWORD=secret

# JWT
JWT_SECRET=your-256-bit-secret-key-here-change-in-production
JWT_ACCESS_TTL=900
JWT_REFRESH_TTL=604800
JWT_ISSUER=kyuubisoft
JWT_ALGORITHM=HS256

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PREFIX=kyuubisoft:

# Logging
LOG_CHANNEL=daily
LOG_LEVEL=debug
